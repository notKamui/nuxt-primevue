{
  "name": "@notkamui/nuxt-primevue",
  "type": "module",
  "version": "0.4.0",
  "description": "Nuxt module for PrimeVue",
  "author": {
    "name": "notKamui",
    "email": "teillard.jimmy@outlook.fr"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/notKamui/nuxt-primevue.git"
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "vue3",
    "nuxt3",
    "nuxt-primevue"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "run-s lint prepack && pnpm publish --access public && git push --follow-tags",
    "lint": "eslint .",
    "taze": "taze major -I"
  },
  "dependencies": {
    "@nuxt/kit": "^3.4.1",
    "primevue": "^3.26.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@nuxt/module-builder": "^0.3.0",
    "@nuxt/schema": "^3.4.1",
    "@nuxt/test-utils": "^3.4.1",
    "@types/node": "^18.15.11",
    "consola": "^3.0.2",
    "eslint": "^8.38.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.4.1",
    "taze": "^0.9.1"
  }
}
