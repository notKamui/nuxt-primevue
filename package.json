{
  "name": "@notkamui/nuxt-primevue",
  "type": "module",
  "version": "0.4.1",
  "description": "Nuxt module for PrimeVue",
  "author": {
    "name": "notKamui",
    "email": "teillard.jimmy@outlook.fr"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/notKamui/nuxt-primevue.git"
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "vue3",
    "nuxt3",
    "nuxt-primevue"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "run-s lint prepack && pnpm publish --access public && git push --follow-tags",
    "lint": "eslint .",
    "taze": "taze major -I"
  },
  "peerDependencies": {
    "primevue": "^3.0.0"
  },
  "dependencies": {
    "@nuxt/kit": "^3.5.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.5.3",
    "@nuxt/test-utils": "^3.5.3",
    "@types/node": "^18.16.18",
    "consola": "^3.1.0",
    "eslint": "^8.43.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.5.3",
    "taze": "^0.10.2"
  }
}
